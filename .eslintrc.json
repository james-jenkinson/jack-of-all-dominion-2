{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [{
        "files": ["*.ts", "*.tsx"]
    }],
    "ignorePatterns": [
        "dev-dist/*",
        "vite.config.ts",
        "vite-env.d.ts"
    ],
    "extends": [
        "love"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-absolute-path": "off",
        "@typescript-eslint/no-non-null-assertion": "off"
    }
}
